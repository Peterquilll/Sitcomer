import random

import requests

def get_episode():
    response = requests.get('http://www.omdbapi.com/?i=tt0108778&season=1&y=2000&apikey=b6762ffb')
    data = response.json()
    episodes = data['Episodes']
    return random.choice(episodes)

def main():
    print(str(get_episode()))


if __name__ == '__main__':
    main()
