import MySQLdb

import requests


db = db = MySQLdb.connect(host="localhost", user="root",
                          passwd="Gospeed69", db="sitcomer")
cursor = db.cursor()
cursor.execute("SELECT * FROM season");

response = requests.get('http://www.omdbapi.com/?i=tt0108778&season=1&y=2000&apikey=b6762ffb')
data = response.json()
episodes = data['Episodes']

def main():
    for episode in episodes:
        query = "INSERT INTO episode(season_number,episode_number,episode_name, imdb_id) VALUES ('1', '" + episode['Episode'] + "', '" + episode['Title'] + "', '" + episode['imdbID'] + "')"
        cursor.execute(query)

db.commit()

if __name__ == '__main__':
    main()

